		{
			"name": "register",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"pm.test(\"Response time is less than 500ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(500);",
							"});",
							"var jsonData = pm.response.json();",
							"var email = pm.environment.get('$randomEmail')",
							"var firstName = pm.environment.get('userFirstName')",
							"var lastName = pm.environment.get('userLastName')",
							"pm.environment.set(\"userId\", jsonData.user.id)",
							"",
							"pm.test(\"Check created user data\", function () {",
							"    pm.expect(jsonData.user.first_name).to.eql(firstName);",
							"    pm.expect(jsonData.user.last_name).to.eql(lastName);",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							""
						],
